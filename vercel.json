{
    "version": 2,
    "builds": [
        {
            "src": "mysite/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9",
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/ws/training_progress/",
            "dest": "mysite/wsgi.py"
        },
        {
            "src": "/(.*)",
            "dest": "mysite/wsgi.py"
        }
    ]
}
